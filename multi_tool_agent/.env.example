# Example environment variables for GCP Agent
# Copy this file to .env and replace with your actual values

# Your Google API key for Gemini model access
GOOGLE_API_KEY=your_google_api_key_here

# The Gemini model to use
MODEL_NAME=gemini-2.0-flash

# Full path to your gcloud executable
# Windows example: C:\Users\username\AppData\Local\Google\Cloud SDK\google-cloud-sdk\bin\gcloud.cmd
# macOS/Linux example: /usr/local/bin/gcloud
GCLOUD_PATH=path_to_your_gcloud_executable 